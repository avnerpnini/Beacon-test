{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Launch localhost",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080/www/",
            "webRoot": "${workspaceFolder}/wwwroot"
            //http-server the comand to start the server            
        },
        {
            "name": "Launch index.html",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": false,
            "file": "${workspaceFolder}/www/index.html"
        },
    ]
}